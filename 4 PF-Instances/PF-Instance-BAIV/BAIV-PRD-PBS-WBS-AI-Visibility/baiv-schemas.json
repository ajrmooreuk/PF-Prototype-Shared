{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://yourdomain.com/schemas/baiv-v2.json",
  "title": "BAIV Agent Orchestrator Schemas",
  "version": "2.0.0",
  "description": "Complete schema definitions for P1-P7 BAIV processes with Schema.org mappings for SEO optimization",
  
  "definitions": {
    
    "BAIVProcess": {
      "type": "object",
      "description": "Base process schema for P1-P7 BAIV",
      "properties": {
        "@context": {
          "type": "string",
          "const": "https://schema.org"
        },
        "@type": {
          "type": "string",
          "enum": ["Action", "SearchAction", "AnalyzeAction", "CreateAction", "AssessAction"]
        },
        "identifier": {
          "type": "string",
          "format": "uuid"
        },
        "name": {
          "type": "string",
          "enum": ["P1_Configuration", "P2_Discovery", "P3_Capture", "P4_Audit", "P5_Analytics", "P6_GapAnalysis", "P7_Ideation"]
        },
        "agent": {
          "$ref": "#/definitions/Agent"
        },
        "startTime": {
          "type": "string",
          "format": "date-time"
        },
        "endTime": {
          "type": "string",
          "format": "date-time"
        },
        "status": {
          "type": "string",
          "enum": ["pending", "running", "completed", "failed"]
        },
        "result": {
          "type": "object"
        }
      },
      "required": ["@context", "@type", "identifier", "name", "agent", "startTime", "status"]
    },
    
    "Agent": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "const": "SoftwareApplication"
        },
        "identifier": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "applicationCategory": {
          "type": "string",
          "enum": ["configuration", "discovery", "capture", "audit", "analytics", "gap_analysis", "ideation"]
        },
        "version": {
          "type": "string"
        }
      },
      "required": ["@type", "identifier", "name", "applicationCategory"]
    },
    
    "P1_Configuration": {
      "allOf": [
        {"$ref": "#/definitions/BAIVProcess"},
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "Action"
            },
            "name": {
              "const": "P1_Configuration"
            },
            "object": {
              "$ref": "#/definitions/BusinessProfile"
            },
            "result": {
              "$ref": "#/definitions/ConfigurationResult"
            }
          }
        }
      ]
    },
    
    "BusinessProfile": {
      "type": "object",
      "properties": {
        "@context": {
          "type": "string",
          "const": "https://schema.org"
        },
        "@type": {
          "type": "string",
          "const": "Organization"
        },
        "name": {
          "type": "string",
          "description": "Company name"
        },
        "url": {
          "type": "string",
          "format": "uri",
          "description": "Company website"
        },
        "description": {
          "type": "string",
          "description": "Business description"
        },
        "industry": {
          "type": "string",
          "description": "Industry vertical"
        },
        "address": {
          "type": "object",
          "properties": {
            "@type": {"const": "PostalAddress"},
            "addressCountry": {"type": "string"},
            "addressRegion": {"type": "string"}
          }
        },
        "sameAs": {
          "type": "array",
          "items": {"type": "string", "format": "uri"},
          "description": "Social media profiles"
        },
        "knowsAbout": {
          "type": "array",
          "items": {"type": "string"},
          "description": "Products/services"
        },
        "competitors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Competitor"
          }
        },
        "targetKeywords": {
          "type": "array",
          "items": {"type": "string"}
        },
        "targetAudience": {
          "$ref": "#/definitions/Audience"
        }
      },
      "required": ["@context", "@type", "name", "url", "industry"]
    },
    
    "Competitor": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Organization"
        },
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "aiVisibilityScore": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "relationship": {
          "type": "string",
          "enum": ["direct", "indirect", "thought_leader"]
        }
      },
      "required": ["@type", "name", "url"]
    },
    
    "Audience": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Audience"
        },
        "audienceType": {
          "type": "string",
          "description": "B2B, B2C, etc."
        },
        "geographicArea": {
          "type": "array",
          "items": {"type": "string"}
        },
        "jobTitle": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    
    "ConfigurationResult": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Thing"
        },
        "workspaceId": {
          "type": "string",
          "format": "uuid"
        },
        "businessProfile": {
          "$ref": "#/definitions/BusinessProfile"
        },
        "integrations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Integration"
          }
        },
        "monitoringSetup": {
          "$ref": "#/definitions/MonitoringSetup"
        }
      },
      "required": ["@type", "workspaceId", "businessProfile", "integrations"]
    },
    
    "Integration": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "SoftwareApplication"
        },
        "name": {
          "type": "string",
          "description": "Integration name (reddit, gsc, ga4, etc.)"
        },
        "applicationCategory": {
          "type": "string",
          "enum": ["seo", "analytics", "social", "content", "research"]
        },
        "status": {
          "type": "string",
          "enum": ["connected", "pending", "error"]
        },
        "connectedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": ["@type", "name", "applicationCategory", "status"]
    },
    
    "MonitoringSetup": {
      "type": "object",
      "properties": {
        "aiAgents": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["chatgpt", "claude", "perplexity", "gemini", "copilot"]
          }
        },
        "alertThreshold": {
          "type": "number",
          "description": "Change threshold for alerts"
        },
        "reportFrequency": {
          "type": "string",
          "enum": ["daily", "weekly", "monthly"]
        }
      }
    },
    
    "P2_Discovery": {
      "allOf": [
        {"$ref": "#/definitions/BAIVProcess"},
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "SearchAction"
            },
            "name": {
              "const": "P2_Discovery"
            },
            "result": {
              "$ref": "#/definitions/DiscoveryResult"
            }
          }
        }
      ]
    },
    
    "DiscoveryResult": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "SearchResultsPage"
        },
        "redditInsights": {
          "$ref": "#/definitions/RedditInsights"
        },
        "aiVisibilityProfile": {
          "$ref": "#/definitions/AIVisibilityProfile"
        },
        "competitorIntelligence": {
          "$ref": "#/definitions/CompetitorIntelligence"
        },
        "searchIntentMap": {
          "$ref": "#/definitions/SearchIntentMap"
        },
        "opportunitiesIdentified": {
          "type": "integer",
          "minimum": 0
        }
      },
      "required": ["@type", "redditInsights", "aiVisibilityProfile", "opportunitiesIdentified"]
    },
    
    "RedditInsights": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "AnalysisNewsArticle"
        },
        "headline": {
          "type": "string",
          "description": "Summary of Reddit findings"
        },
        "totalMentions": {
          "type": "integer"
        },
        "subredditsMonitored": {
          "type": "array",
          "items": {"type": "string"}
        },
        "sentimentBreakdown": {
          "type": "object",
          "properties": {
            "positive": {"type": "number", "minimum": 0, "maximum": 1},
            "neutral": {"type": "number", "minimum": 0, "maximum": 1},
            "negative": {"type": "number", "minimum": 0, "maximum": 1}
          }
        },
        "keyThemes": {
          "type": "array",
          "items": {"type": "string"}
        },
        "painPoints": {
          "type": "array",
          "items": {"type": "string"}
        },
        "competitorMentions": {
          "type": "object",
          "additionalProperties": {"type": "integer"}
        }
      },
      "required": ["@type", "headline", "totalMentions", "sentimentBreakdown"]
    },
    
    "AIVisibilityProfile": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Report"
        },
        "name": {
          "const": "AI Visibility Assessment"
        },
        "brandMentionRate": {
          "type": "object",
          "properties": {
            "chatgpt": {"type": "number", "minimum": 0, "maximum": 1},
            "claude": {"type": "number", "minimum": 0, "maximum": 1},
            "perplexity": {"type": "number", "minimum": 0, "maximum": 1}
          }
        },
        "averagePosition": {
          "type": "number",
          "description": "Average position when mentioned (1 = first)"
        },
        "citationSources": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Citation"
          }
        },
        "visibilityScore": {
          "type": "number",
          "minimum": 0,
          "maximum": 100,
          "description": "Overall AI visibility score"
        },
        "gapsIdentified": {
          "type": "array",
          "items": {"type": "string"}
        }
      },
      "required": ["@type", "name", "brandMentionRate", "visibilityScore"]
    },
    
    "Citation": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "enum": ["CreativeWork", "WebPage", "ScholarlyArticle"]
        },
        "source": {
          "type": "string",
          "description": "Source domain"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "count": {
          "type": "integer",
          "description": "Number of times cited"
        },
        "authority": {
          "type": "string",
          "enum": ["high", "medium", "low"]
        }
      },
      "required": ["@type", "source", "count"]
    },
    
    "CompetitorIntelligence": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Report"
        },
        "name": {
          "const": "Competitive Analysis"
        },
        "competitors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/CompetitorProfile"
          }
        },
        "shareOfVoice": {
          "type": "object",
          "additionalProperties": {"type": "number"}
        }
      }
    },
    
    "CompetitorProfile": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Organization"
        },
        "name": {"type": "string"},
        "url": {"type": "string", "format": "uri"},
        "contentStrategy": {
          "type": "object",
          "properties": {
            "totalPages": {"type": "integer"},
            "blogPosts": {"type": "integer"},
            "schemaUsage": {"type": "number", "minimum": 0, "maximum": 1}
          }
        },
        "topPerformingContent": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ContentItem"
          }
        }
      }
    },
    
    "SearchIntentMap": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Map"
        },
        "queries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SearchQuery"
          }
        },
        "intentDistribution": {
          "type": "object",
          "properties": {
            "informational": {"type": "number"},
            "navigational": {"type": "number"},
            "transactional": {"type": "number"},
            "commercial": {"type": "number"}
          }
        }
      }
    },
    
    "SearchQuery": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "SearchAction"
        },
        "query": {"type": "string"},
        "searchVolume": {"type": "integer"},
        "intent": {
          "type": "string",
          "enum": ["informational", "navigational", "transactional", "commercial"]
        },
        "currentRanking": {"type": "integer"},
        "difficulty": {
          "type": "string",
          "enum": ["easy", "medium", "hard"]
        }
      }
    },
    
    "P3_Capture": {
      "allOf": [
        {"$ref": "#/definitions/BAIVProcess"},
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "AnalyzeAction"
            },
            "name": {
              "const": "P3_Capture"
            },
            "result": {
              "$ref": "#/definitions/CaptureResult"
            }
          }
        }
      ]
    },
    
    "CaptureResult": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Dataset"
        },
        "paaGraph": {
          "$ref": "#/definitions/PAAGraph"
        },
        "contentInventory": {
          "$ref": "#/definitions/ContentInventory"
        },
        "coverageMap": {
          "$ref": "#/definitions/CoverageMap"
        },
        "schemaAudit": {
          "$ref": "#/definitions/SchemaAudit"
        },
        "gapsIdentified": {
          "type": "integer"
        }
      },
      "required": ["@type", "paaGraph", "contentInventory", "gapsIdentified"]
    },
    
    "PAAGraph": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "CreativeWork"
        },
        "name": {
          "const": "People Also Ask Question Graph"
        },
        "totalQuestions": {
          "type": "integer"
        },
        "rootKeywords": {
          "type": "integer"
        },
        "maxDepth": {
          "type": "integer",
          "description": "Maximum recursion depth"
        },
        "clusters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/QuestionCluster"
          }
        },
        "questions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PAAQuestion"
          }
        }
      },
      "required": ["@type", "name", "totalQuestions", "clusters"]
    },
    
    "QuestionCluster": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Thing"
        },
        "theme": {
          "type": "string",
          "description": "Cluster theme"
        },
        "questions": {
          "type": "integer",
          "description": "Number of questions in cluster"
        },
        "avgSearchVolume": {
          "type": "integer"
        }
      }
    },
    
    "PAAQuestion": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Question"
        },
        "text": {
          "type": "string"
        },
        "parent": {
          "type": "string",
          "description": "Parent question or root keyword"
        },
        "depth": {
          "type": "integer"
        },
        "searchVolume": {
          "type": "integer"
        },
        "answeredBy": {
          "type": "array",
          "items": {"type": "string"},
          "description": "URLs of pages answering this"
        }
      },
      "required": ["@type", "text"]
    },
    
    "ContentInventory": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Collection"
        },
        "totalPages": {
          "type": "integer"
        },
        "contentTypes": {
          "type": "object",
          "properties": {
            "blogPosts": {"type": "integer"},
            "productPages": {"type": "integer"},
            "landingPages": {"type": "integer"},
            "faqPages": {"type": "integer"}
          }
        },
        "schemaCoverage": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "description": "% of pages with schema"
        },
        "pages": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ContentItem"
          }
        }
      }
    },
    
    "ContentItem": {
      "type": "object",
      "properties": {
        "@context": {
          "type": "string",
          "const": "https://schema.org"
        },
        "@type": {
          "type": "string",
          "enum": ["Article", "BlogPosting", "WebPage", "FAQPage", "HowTo", "Product"]
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "headline": {
          "type": "string"
        },
        "wordCount": {
          "type": "integer"
        },
        "datePublished": {
          "type": "string",
          "format": "date"
        },
        "author": {
          "type": "object",
          "properties": {
            "@type": {"const": "Person"},
            "name": {"type": "string"}
          }
        },
        "schemaTypes": {
          "type": "array",
          "items": {"type": "string"}
        },
        "entities": {
          "type": "array",
          "items": {"type": "string"}
        },
        "internalLinks": {
          "type": "integer"
        },
        "externalLinks": {
          "type": "integer"
        },
        "performanceMetrics": {
          "$ref": "#/definitions/ContentPerformance"
        }
      },
      "required": ["@context", "@type", "url", "headline"]
    },
    
    "ContentPerformance": {
      "type": "object",
      "properties": {
        "pageViews": {"type": "integer"},
        "avgTimeOnPage": {"type": "string"},
        "bounceRate": {"type": "number"},
        "backlinks": {"type": "integer"},
        "socialShares": {"type": "integer"}
      }
    },
    
    "CoverageMap": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Map"
        },
        "questionsCovered": {
          "type": "integer"
        },
        "questionsUncovered": {
          "type": "integer"
        },
        "coverageRate": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "topGaps": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    
    "SchemaAudit": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Report"
        },
        "name": {
          "const": "Schema Markup Audit"
        },
        "totalPages": {"type": "integer"},
        "pagesWithSchema": {"type": "integer"},
        "schemaTypes": {
          "type": "object",
          "additionalProperties": {"type": "integer"}
        },
        "errors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SchemaError"
          }
        },
        "opportunities": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    
    "SchemaError": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Thing"
        },
        "url": {"type": "string", "format": "uri"},
        "schemaType": {"type": "string"},
        "errorType": {
          "type": "string",
          "enum": ["missing_required", "invalid_property", "wrong_type"]
        },
        "errorMessage": {"type": "string"}
      }
    },
    
    "P4_Audit": {
      "allOf": [
        {"$ref": "#/definitions/BAIVProcess"},
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "AssessAction"
            },
            "name": {
              "const": "P4_Audit"
            },
            "result": {
              "$ref": "#/definitions/AuditResult"
            }
          }
        }
      ]
    },
    
    "AuditResult": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Report"
        },
        "name": {
          "const": "BAIV Brand Audit"
        },
        "overallBAIVScore": {
          "type": "number",
          "minimum": 0,
          "maximum": 100
        },
        "scoreBreakdown": {
          "type": "object",
          "properties": {
            "aiVisibilityScore": {"type": "number", "minimum": 0, "maximum": 100},
            "contentQualityScore": {"type": "number", "minimum": 0, "maximum": 100},
            "technicalSEOScore": {"type": "number", "minimum": 0, "maximum": 100},
            "brandConsistencyScore": {"type": "number", "minimum": 0, "maximum": 100}
          }
        },
        "competitiveBenchmark": {
          "type": "object",
          "additionalProperties": {"type": "number"}
        },
        "keyFindings": {
          "type": "array",
          "items": {"type": "string"}
        },
        "recommendations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Recommendation"
          }
        }
      },
      "required": ["@type", "name", "overallBAIVScore", "scoreBreakdown", "recommendations"]
    },
    
    "Recommendation": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "Action"
        },
        "priority": {
          "type": "string",
          "enum": ["high", "medium", "low"]
        },
        "category": {
          "type": "string",
          "enum": ["AI_Visibility", "Content_Quality", "Technical_SEO", "Schema_Markup", "Brand_Consistency"]
        },
        "action": {
          "type": "string"
        },
        "estimatedImpact": {
          "type": "string",
          "description": "Impact on BAIV score (e.g., '+15 points')"
        },
        "estimatedEffort": {
          "type": "string",
          "description": "Time estimate (e.g., '8-12 hours')"
        }
      },
      "required": ["@type", "priority", "category", "action"]
    },
    
    "P7_Ideation": {
      "allOf": [
        {"$ref": "#/definitions/BAIVProcess"},
        {
          "type": "object",
          "properties": {
            "@type": {
              "const": "CreateAction"
            },
            "name": {
              "const": "P7_Ideation"
            },
            "result": {
              "$ref": "#/definitions/IdeationResult"
            }
          }
        }
      ]
    },
    
    "IdeationResult": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "CreativeWork"
        },
        "name": {
          "const": "Content Strategy Plan"
        },
        "contentIdeas": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ContentIdea"
          }
        },
        "contentClusters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ContentCluster"
          }
        },
        "contentCalendar": {
          "$ref": "#/definitions/ContentCalendar"
        },
        "totalIdeas": {
          "type": "integer"
        },
        "estimatedTotalEffort": {
          "type": "number",
          "description": "Total hours"
        }
      },
      "required": ["@type", "name", "contentIdeas", "totalIdeas"]
    },
    
    "ContentIdea": {
      "type": "object",
      "properties": {
        "@context": {
          "type": "string",
          "const": "https://schema.org"
        },
        "@type": {
          "type": "string",
          "enum": ["Article", "BlogPosting", "HowTo", "FAQPage"]
