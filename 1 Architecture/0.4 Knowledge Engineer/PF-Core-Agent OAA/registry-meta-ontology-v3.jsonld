  "businessRules": [
    {
      "identifier": "rule-reg-001",
      "name": "UniqueOntologyIdentifier",
      "description": "Each ontology must have a unique identifier within its tenant",
      "ruleType": "constraint",
      "formalExpression": "FORALL o1, o2: OntologyRegistryEntry WHERE o1.tenant = o2.tenant IMPLIES o1.identifier != o2.identifier",
      "humanReadable": "No two ontologies within the same tenant can have the same identifier",
      "severity": "error",
      "appliesTo": ["reg:OntologyRegistryEntry.identifier", "reg:OntologyRegistryEntry.tenant"],
      "executionContext": "create"
    },
    {
      "identifier": "rule-reg-002",
      "name": "TenantIsolation",
      "description": "Ontologies are isolated by tenant unless explicitly shared",
      "ruleType": "constraint",
      "formalExpression": "FORALL o: OntologyRegistryEntry WHERE o.visibility = 'private' IMPLIES accessible_by(o, o.tenant) ONLY",
      "humanReadable": "Private ontologies can only be accessed by their owning tenant",
      "severity": "error",
      "appliesTo": ["reg:OntologyRegistryEntry.visibility", "reg:OntologyRegistryEntry.tenant"],
      "executionContext": "query"
    },
    {
      "identifier": "rule-reg-003",
      "name": "MinimumEntityCount",
      "description": "Ontology must define at least one entity",
      "ruleType": "validation",
      "formalExpression": "FORALL o: OntologyRegistryEntry IMPLIES COUNT(o.entities) >= 1",
      "humanReadable": "Every ontology must define at least one entity",
      "severity": "error",
      "appliesTo": ["reg:OntologyRegistryEntry.entities"],
      "executionContext": "create"
    },
    {
      "identifier": "rule-reg-004",
      "name": "NoCyclicDependencies",
      "description": "Ontology dependencies must not form cycles",
      "ruleType": "validation",
      "formalExpression": "FORALL o: OntologyRegistryEntry IMPLIES NOT EXISTS path: o -[dependsOn*]-> o",
      "humanReadable": "Ontologies cannot have circular dependencies",
      "severity": "error",
      "appliesTo": ["reg:OntologyRegistryEntry.dependencies"],
      "executionContext": "create"
    },
    {
      "identifier": "rule-reg-005",
      "name": "SchemaOrgGrounding",
      "description": "At least 80% of entities should be grounded in schema.org",
      "ruleType": "validation",
      "formalExpression": "FORALL o: OntologyRegistryEntry IMPLIES (COUNT(o.entities WHERE schemaOrgBase != null) / COUNT(o.entities)) >= 0.80",
      "humanReadable": "Ontology should have at least 80% schema.org alignment for quality",
      "severity": "warning",
      "appliesTo": ["reg:OntologyEntity.schemaOrgBase"],
      "executionContext": "create"
    }
  ],
  
