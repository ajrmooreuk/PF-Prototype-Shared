  "cascadeDefinition": {
    "name": "Value Engineering Cascade",
    "version": "1.0.0",
    "layers": [
      {
        "layer": 1,
        "name": "VSOM Strategic Foundation",
        "ontology": "pf-core:ontology:vsom-v1",
        "entities": ["Vision", "Mission", "StrategicObjective", "OperationalStrategy", "Metric"]
      },
      {
        "layer": 2,
        "name": "RACI Accountability Matrix",
        "ontology": "pf-core:ontology:roles-raci-rbac-v3",
        "entities": ["ExecutiveRole", "RACIAssignment", "BusinessRole"]
      },
      {
        "layer": 3,
        "name": "OKR Domain Scoping",
        "ontology": "pf-core:ontology:okr-v1",
        "entities": ["Objective", "KeyResult", "TimePeriod", "AlignmentScore"]
      },
      {
        "layer": 4,
        "name": "Value Proposition Module",
        "ontology": "pf-core:ontology:value-proposition-v1",
        "entities": ["ValueProposition", "CustomerSegment", "ProblemDefinition", "ValueDriver", "Hypothesis"]
      },
      {
        "layer": 5,
        "name": "Context Engineering",
        "description": "Cross-ontology context assembly for AI agents",
        "components": ["MarketContext", "OrganizationalContext", "CompetitiveContext"]
      },
      {
        "layer": 6,
        "name": "PRD Generation",
        "description": "Automated requirements documentation from value proposition",
        "outputs": ["PRD", "UserStories", "AcceptanceCriteria", "TDDSpecifications"]
      },
      {
        "layer": 7,
        "name": "TDD-Driven Execution",
        "description": "Test-first implementation with 80%+ coverage target",
        "outputs": ["TestSpecifications", "Implementation", "BenefitsValidation"]
      }
    ],
    "diagram": "diagrams/value-engineering-cascade.mermaid"
  }
