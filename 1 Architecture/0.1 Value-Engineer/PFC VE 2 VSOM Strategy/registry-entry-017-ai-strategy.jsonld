    ],
    
    "entities": [
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Capability",
        "name": "Capability",
        "description": "An organizational ability to perform specific activities or deliver value",
        "schemaOrgBase": "schema:Thing",
        "propertyCount": 11
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Gap",
        "name": "Gap",
        "description": "A deficiency between current and desired state",
        "schemaOrgBase": "schema:Thing",
        "propertyCount": 12
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Position",
        "name": "Position",
        "description": "Strategic market position relative to competitors",
        "schemaOrgBase": "schema:Thing",
        "propertyCount": 12
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Opportunity",
        "name": "Opportunity",
        "description": "A strategic opportunity for value creation or competitive advantage",
        "schemaOrgBase": "schema:Thing",
        "propertyCount": 15
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Organization",
        "name": "Organization",
        "description": "An organization that possesses capabilities and occupies positions",
        "schemaOrgBase": "schema:Organization",
        "propertyCount": 10
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Competitor",
        "name": "Competitor",
        "description": "A competing organization with competitive attributes",
        "schemaOrgBase": "baiv:ai-strategy:Organization",
        "propertyCount": 6
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Market",
        "name": "Market",
        "description": "A market segment or domain where organizations compete",
        "schemaOrgBase": "schema:Thing",
        "propertyCount": 9
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Initiative",
        "name": "Initiative",
        "description": "A strategic initiative to address gaps or pursue opportunities",
        "schemaOrgBase": "schema:Action",
        "propertyCount": 12
      },
      {
        "@type": "EntityDefinition",
        "@id": "baiv:ai-strategy:Technology",
        "name": "Technology",
        "description": "A technology that enables organizational capabilities",
        "schemaOrgBase": "schema:SoftwareApplication",
        "propertyCount": 9
      }
    ],
    
    "relationships": [
      {"name": "hasCapability", "domain": "Organization", "range": "Capability", "cardinality": "1:*"},
      {"name": "revealsGap", "domain": "Capability", "range": "Gap", "cardinality": "1:*"},
      {"name": "createsOpportunity", "domain": "Gap", "range": "Opportunity", "cardinality": "*:*"},
      {"name": "requiresCapability", "domain": "Opportunity", "range": "Capability", "cardinality": "*:*"},
      {"name": "occupiesPosition", "domain": "Organization", "range": "Position", "cardinality": "1:*"},
      {"name": "positionedIn", "domain": "Position", "range": "Market", "cardinality": "*:1"},
      {"name": "competesAgainst", "domain": "Organization", "range": "Competitor", "cardinality": "*:*"},
      {"name": "relativeToCompetitor", "domain": "Position", "range": "Competitor", "cardinality": "*:*"},
      {"name": "competesInMarket", "domain": "Competitor", "range": "Market", "cardinality": "*:*"},
      {"name": "dependsOnCapability", "domain": "Capability", "range": "Capability", "cardinality": "*:*"},
      {"name": "enabledByTechnology", "domain": "Capability", "range": "Technology", "cardinality": "*:*"},
      {"name": "addressesGap", "domain": "Initiative", "range": "Gap", "cardinality": "*:*"},
      {"name": "pursuesOpportunity", "domain": "Initiative", "range": "Opportunity", "cardinality": "*:*"},
      {"name": "buildsCapability", "domain": "Initiative", "range": "Capability", "cardinality": "*:*"},
      {"name": "hasTechnologyStack", "domain": "Organization", "range": "Technology", "cardinality": "1:*"},
      {"name": "influencesPosition", "domain": "Capability", "range": "Position", "cardinality": "*:*"}
    ],
    
    "businessRules": [
      {
        "ruleId": "BR-001",
        "name": "Capability Maturity Progression",
        "description": "Maturity must follow progression: Initial → Developing → Defined → Managed → Optimizing",
        "severity": "Error"
      },
      {
        "ruleId": "BR-002",
        "name": "Gap Requires Current and Desired State",
        "description": "Every gap must define both current state and desired state",
        "severity": "Error"
      },
      {
        "ruleId": "BR-003",
        "name": "Critical Gaps Must Have Initiatives",
        "description": "All gaps with severity='Critical' must have at least one initiative addressing them",
        "severity": "Warning"
      },
      {
        "ruleId": "BR-004",
        "name": "Opportunity Feasibility vs Priority Alignment",
        "description": "High priority opportunities should have medium or high feasibility",
        "severity": "Warning"
      },
      {
        "ruleId": "BR-005",
        "name": "Position Must Reference Market",
        "description": "Every position must be associated with a specific market segment",
        "severity": "Error"
      },
      {
        "ruleId": "BR-006",
        "name": "Capability Dependency Acyclicity",
        "description": "Capability dependencies must not create circular dependencies",
        "severity": "Error"
      },
      {
        "ruleId": "BR-007",
        "name": "Organization Type Consistency",
        "description": "An organization cannot be both 'Self' and 'Competitor' simultaneously",
        "severity": "Error"
      },
      {
        "ruleId": "BR-008",
        "name": "Direct Competitor Must Have Market Overlap",
        "description": "Competitors classified as 'Direct' must have at least one market overlap",
        "severity": "Warning"
      },
      {
        "ruleId": "BR-009",
        "name": "Initiative Timeline Validation",
        "description": "Initiative end date must be after start date",
        "severity": "Error"
      },
      {
        "ruleId": "BR-010",
        "name": "Technology Adoption Prerequisite",
        "description": "Technologies must be 'Adopted' or 'Standard' to enable capabilities at 'Managed' or 'Optimizing' maturity",
        "severity": "Warning"
      },
      {
        "ruleId": "BR-011",
        "name": "Assessment Date Recency",
        "description": "Capability and Position assessments older than 12 months should be flagged for review",
        "severity": "Info"
      },
      {
        "ruleId": "BR-012",
        "name": "Opportunity Value Justification",
        "description": "High priority opportunities should have estimated potential value documented",
        "severity": "Warning"
      }
    ],
    
    "aiAgentCapabilities": {
