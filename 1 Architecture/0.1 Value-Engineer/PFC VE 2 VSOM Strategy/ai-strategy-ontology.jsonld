  "businessRules": [
    {
      "@type": "BusinessRule",
      "ruleId": "BR-001",
      "name": "Capability Maturity Progression",
      "description": "Capability maturity must follow natural progression: Initial → Developing → Defined → Managed → Optimizing",
      "ruleType": "Constraint",
      "severity": "Error",
      "scope": ["Capability"],
      "validation": "Maturity level changes must be sequential, cannot skip levels"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-002",
      "name": "Gap Requires Current and Desired State",
      "description": "Every gap must define both current state and desired state",
      "ruleType": "Mandatory",
      "severity": "Error",
      "scope": ["Gap"],
      "validation": "currentState and desiredState fields must be non-empty"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-003",
      "name": "Critical Gaps Must Have Initiatives",
      "description": "All gaps with severity='Critical' must have at least one initiative addressing them",
      "ruleType": "Business Logic",
      "severity": "Warning",
      "scope": ["Gap", "Initiative"],
      "validation": "For each Gap where severity='Critical', exists at least one Initiative where addressesGap includes that Gap"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-004",
      "name": "Opportunity Feasibility vs Priority Alignment",
      "description": "High priority opportunities should have medium or high feasibility",
      "ruleType": "Business Logic",
      "severity": "Warning",
      "scope": ["Opportunity"],
      "validation": "If priority='Critical' or 'High', feasibility should not be 'Low'"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-005",
      "name": "Position Must Reference Market",
      "description": "Every position must be associated with a specific market segment",
      "ruleType": "Mandatory",
      "severity": "Error",
      "scope": ["Position"],
      "validation": "marketSegment field must reference valid Market entity"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-006",
      "name": "Capability Dependency Acyclicity",
      "description": "Capability dependencies must not create circular dependencies",
      "ruleType": "Constraint",
      "severity": "Error",
      "scope": ["Capability"],
      "validation": "Graph of dependsOnCapability relationships must be acyclic (DAG)"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-007",
      "name": "Organization Type Consistency",
      "description": "An organization cannot be both 'Self' and 'Competitor' simultaneously",
      "ruleType": "Constraint",
      "severity": "Error",
      "scope": ["Organization"],
      "validation": "organizationType='Self' excludes being Competitor subtype"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-008",
      "name": "Direct Competitor Must Have Market Overlap",
      "description": "Competitors classified as 'Direct' must have at least one market overlap",
      "ruleType": "Business Logic",
      "severity": "Warning",
      "scope": ["Competitor"],
      "validation": "If competitorType='Direct', marketOverlap array must be non-empty"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-009",
      "name": "Initiative Timeline Validation",
      "description": "Initiative end date must be after start date",
      "ruleType": "Constraint",
      "severity": "Error",
      "scope": ["Initiative"],
      "validation": "If both startDate and endDate exist, endDate > startDate"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-010",
      "name": "Technology Adoption Prerequisite",
      "description": "Technologies must be 'Adopted' or 'Standard' to enable capabilities marked as 'Managed' or 'Optimizing' maturity",
      "ruleType": "Business Logic",
      "severity": "Warning",
      "scope": ["Capability", "Technology"],
      "validation": "If Capability.maturityLevel in ['Managed', 'Optimizing'], then Technology.adoptionStatus must be in ['Adopted', 'Standard']"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-011",
      "name": "Assessment Date Recency",
      "description": "Capability and Position assessments older than 12 months should be flagged for review",
      "ruleType": "Quality",
      "severity": "Info",
      "scope": ["Capability", "Position"],
      "validation": "If dateAssessed or assessmentDate is more than 365 days in past, flag for review"
    },
    {
      "@type": "BusinessRule",
      "ruleId": "BR-012",
      "name": "Opportunity Value Justification",
      "description": "High priority opportunities should have estimated potential value documented",
      "ruleType": "Quality",
      "severity": "Warning",
      "scope": ["Opportunity"],
      "validation": "If priority='Critical' or 'High', potentialValue should be specified"
    }
  ],
  
  "aiAgentCapabilities": {
    "queryPatterns": [
      {
        "pattern": "Gap Identification",
        "description": "Identify gaps by comparing current vs target capability states",
        "sparqlExample": "SELECT ?cap ?gap WHERE { ?org hasCapability ?cap . ?cap revealsGap ?gap . ?gap severity 'Critical' }"
      },
      {
        "pattern": "Competitive Analysis",
        "description": "Analyze competitive positioning across markets",
        "sparqlExample": "SELECT ?org ?position ?competitor ?market WHERE { ?org occupiesPosition ?position . ?position positionedIn ?market . ?position relativeToCompetitor ?competitor }"
      },
      {
        "pattern": "Opportunity Scoring",
        "description": "Score and prioritize opportunities based on value, feasibility, and strategic fit",
        "sparqlExample": "SELECT ?opp ?value ?feasibility ?priority WHERE { ?opp a Opportunity . ?opp potentialValue ?value . ?opp feasibility ?feasibility . ?opp priority ?priority } ORDER BY DESC(?priority) DESC(?value)"
      },
      {
        "pattern": "Capability Dependency Mapping",
        "description": "Map capability dependencies to identify foundational requirements",
        "sparqlExample": "SELECT ?cap ?dependsOn WHERE { ?cap dependsOnCapability ?dependsOn } ORDER BY ?cap"
      },
      {
        "pattern": "Technology Stack Analysis",
        "description": "Analyze technology stacks and their capability enablement",
        "sparqlExample": "SELECT ?org ?tech ?cap WHERE { ?org hasTechnologyStack ?tech . ?tech enablesCapability ?cap }"
      },
      {
        "pattern": "Strategic Initiative Portfolio",
        "description": "View all initiatives and what they address/build",
        "sparqlExample": "SELECT ?initiative ?gap ?opportunity ?capability WHERE { ?initiative addressesGap ?gap . OPTIONAL { ?initiative pursuesOpportunity ?opportunity } . OPTIONAL { ?initiative buildsCapability ?capability } }"
      }
    ],
    "reasoningCapabilities": [
      {
        "capability": "Gap-to-Opportunity Inference",
        "description": "Infer opportunities from identified gaps and market conditions",
        "logic": "IF Gap.severity='Critical' AND Market.growthRate>10% AND Competitor.competitiveStrength='Weak' THEN infer Opportunity with high priority"
      },
      {
        "capability": "Capability Sufficiency Assessment",
        "description": "Assess if current capabilities are sufficient for opportunity pursuit",
        "logic": "FOR Opportunity, check IF ALL requiredCapabilities exist with maturityLevel >= 'Defined'"
      },
      {
        "capability": "Competitive Positioning Recommendation",
        "description": "Recommend positioning strategy based on capabilities vs competitors",
        "logic": "Compare Organization.capabilities vs Competitor.capabilities in shared Market to recommend Position.positionType"
      },
      {
        "capability": "Innovation Horizon Classification",
        "description": "Classify opportunities into innovation horizons based on timeframe and feasibility",
        "logic": "Map Opportunity.timeHorizon and Opportunity.feasibility to recommend resource allocation strategy"
      },
      {
        "capability": "Technology Adoption Priority",
        "description": "Prioritize technology adoption based on capability gaps and strategic importance",
        "logic": "FOR Technology, calculate priority score based on: count(enablesCapability where Capability.strategicImportance='Critical') and count(Gap.addressedByInitiative where Initiative requires Technology)"
      }
    ],
    "decisionSupport": [
      {
        "decision": "Opportunity Prioritization",
        "inputs": ["Opportunity.potentialValue", "Opportunity.feasibility", "Opportunity.requiredCapabilities", "Gap.severity"],
        "output": "Ranked list of opportunities with investment recommendations",
        "method": "Multi-criteria decision analysis (MCDA) with weighted scoring"
      },
      {
        "decision": "Capability Investment",
        "inputs": ["Capability.maturityLevel", "Capability.strategicImportance", "Gap.severity", "Opportunity.requiresCapability"],
        "output": "Capability investment priority ranking",
        "method": "Dependency analysis + strategic importance weighting"
      },
      {
        "decision": "Competitive Response",
        "inputs": ["Competitor.recentMoves", "Position.trendDirection", "Market.maturity", "Organization.capabilities"],
        "output": "Recommended competitive response strategy",
        "method": "Game theory + capability gap analysis"
      },
      {
        "decision": "Market Entry Feasibility",
        "inputs": ["Market.barriers", "Market.maturity", "Organization.capabilities", "Competitor.competitiveStrength"],
        "output": "Market entry feasibility score and requirements",
        "method": "Barrier analysis + capability matching"
      }
    ],
    "integrationPoints": [
      {
        "system": "Business Intelligence Platforms",
        "dataFlow": "Import market data, financial metrics, competitive intelligence",
        "frequency": "Weekly/Monthly"
      },
      {
        "system": "Project Management Systems",
        "dataFlow": "Sync Initiative status, timelines, and outcomes",
        "frequency": "Real-time/Daily"
      },
      {
        "system": "Technology Asset Management",
        "dataFlow": "Technology inventory, adoption status, capabilities enabled",
        "frequency": "Monthly"
      },
      {
        "system": "Strategic Planning Tools",
        "dataFlow": "Strategic objectives, OKRs, performance metrics",
        "frequency": "Quarterly"
      }
    ]
  },
  
