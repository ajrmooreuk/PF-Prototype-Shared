          "outputFormat": "Priority matrix with recommendations"
        },
        {
          "toolId": "baseline_gap_calculator",
          "description": "Computes gap between current state and target",
          "inputParams": ["baseline_value", "target_value", "timeframe"],
          "outputFormat": "Gap analysis with milestone recommendations"
        }
      ],
      
      "inputs": {
        "required": [
          {
            "name": "corporate_strategy",
            "type": "vsom:StrategyComponent",
            "source": "strategy_formulation_agent"
          },
          {
            "name": "functional_strategies",
            "type": "schema:ItemList",
            "source": "strategy_formulation_agent"
          }
        ],
        "optional": [
          {
            "name": "resource_availability",
            "type": "schema:MonetaryAmount",
            "description": "Available budget and capacity"
          },
          {
            "name": "organizational_constraints",
            "type": "schema:ItemList",
            "description": "Limitations and constraints"
          },
          {
            "name": "baseline_performance_data",
            "type": "schema:Dataset",
            "description": "Current performance metrics"
          }
        ]
      },
      
      "outputs": {
        "primary": [
          {
            "name": "strategic_objectives_catalogue",
            "type": "schema:ItemList",
            "description": "Complete set of validated objectives"
          },
          {
            "name": "objective_hierarchy_map",
            "type": "schema:Table",
            "description": "Strategic → Tactical → Operational hierarchy"
          }
        ],
        "secondary": [
          {
            "name": "dependency_matrix",
            "type": "schema:Table",
            "description": "Inter-objective dependencies"
          },
          {
            "name": "resource_requirements_report",
            "type": "schema:Report",
            "description": "Resource needs per objective"
          },
          {
            "name": "milestone_timeline",
            "type": "schema:Schedule",
            "description": "Key milestones with dates"
          }
        ]
      },
      
      "integrations": {
        "baivProcess": "P6_GapAnalysis",
        "upstreamAgents": ["strategy_formulation_agent"],
        "downstreamAgents": ["metrics_design_agent", "okr_orchestration_agent"]
      },
      
      "objectiveTypes": {
        "strategic": "Annual enterprise-level goals",
        "tactical": "Quarterly functional-level goals",
        "operational": "Monthly team-level goals"
      },
      
      "developmentPhase": "Phase 2",
      "estimatedEffort": "Medium",
      "priority": 4
    },

