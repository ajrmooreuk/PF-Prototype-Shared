  "ve:RACIXModel": {
    "@type": "rrrr:ResponsibilityModel",
    "rrrr:modelName": "RACI-X Extended",
    "rrrr:modelVersion": "1.1.0",
    "rrrr:roles": [
      {"code": "R", "name": "Responsible", "description": "Does the work to complete the task"},
      {"code": "A", "name": "Accountable", "description": "Ultimately answerable for the activity - ONE per activity"},
      {"code": "C", "name": "Consulted", "description": "Provides input and expertise before decision/action"},
      {"code": "I", "name": "Informed", "description": "Kept up-to-date on progress and outcomes"},
      {"code": "S", "name": "Supporting", "description": "Provides resources/expertise without ownership"},
      {"code": "D", "name": "Delegated", "description": "Accountability retained, responsibility delegated with authority"},
      {"code": "O", "name": "Overlap", "description": "Formal/informal shared responsibility requiring CollaborationProtocol"}
    ],
    "rrrr:validationRules": [
      "Exactly one Accountable (A) per activity",
      "At least one Responsible (R) per activity",
      "Delegated (D) requires explicit DelegationChain definition",
      "Overlap (O) requires CollaborationProtocol specification"
    ]
  },

  "ve:OrganizationSizeDefinitions": {
    "@type": "rrrr:OrganizationSizeTaxonomy",
    "rrrr:sizes": [
      {"code": "MICRO", "name": "Micro", "employeeRange": "1-10", "typicalCSuiteCount": "1-2", "description": "Founder/CEO often covers multiple C-Suite functions"},
      {"code": "SMALL", "name": "Small", "employeeRange": "11-50", "typicalCSuiteCount": "2-4", "description": "Core C-Suite with significant role subsumption"},
      {"code": "MEDIUM", "name": "Medium", "employeeRange": "51-250", "typicalCSuiteCount": "4-8", "description": "Expanded C-Suite with selective specialization"},
      {"code": "LARGE", "name": "Large", "employeeRange": "251-1000", "typicalCSuiteCount": "8-12", "description": "Full C-Suite with most functions represented"},
      {"code": "ENTERPRISE", "name": "Enterprise", "employeeRange": "1000+", "typicalCSuiteCount": "12-20+", "description": "Complete C-Suite with specialized and emerging roles"}
    ]
  },

  "ve:SubsumptionConfiguration": {
    "@type": "rrrr:SubsumptionModel",
    "rrrr:subsumptionTypes": [
      {"code": "FULL", "name": "Full Subsumption", "description": "Complete role absorption - all responsibilities transferred"},
      {"code": "PARTIAL", "name": "Partial Subsumption", "description": "Specific scope transferred, remainder retained or eliminated"},
      {"code": "TEMPORARY", "name": "Temporary Subsumption", "description": "Time-bounded coverage (e.g., interim, parental leave)"},
      {"code": "DELEGATION", "name": "Delegation", "description": "Cascaded authority chain with accountability retained at source"}
    ],
    "rrrr:subsumptionProperties": {
      "scopeTransferred": "Array of specific responsibilities transferred",
      "effectiveDate": "When subsumption takes effect",
      "expirationDate": "When subsumption ends (for TEMPORARY)",
      "retainedBy": "Role retaining accountability in DELEGATION",
      "collaborationProtocol": "How subsumed and subsuming roles collaborate"
    }
  },

  "ve:ManagementTierCascade": {
    "@type": "rrrr:ManagementHierarchy",
    "rrrr:tiers": [
      {"tier": 0, "name": "L0-Enterprise", "typicalRoles": ["CEO", "Board"], "okrLevel": "Enterprise", "accountableTo": "Board/Shareholders"},
      {"tier": 1, "name": "L1-CSuite", "typicalRoles": ["CFO", "COO", "CTO", "CMO", "CPO", "CHRO", "CIO", "CISO", "CDO", "CRO", "CCO", "CLO", "CAO", "CSO", "CGO", "CXO", "CAIO", "CKO", "CBO", "CPrO", "CEthO", "CSustO"], "okrLevel": "Functional", "accountableTo": "L0"},
      {"tier": 2, "name": "L2-Directors", "typicalRoles": ["VP", "Director", "Head of"], "okrLevel": "Departmental", "accountableTo": "L1 (OKR Delivery Owners)"},
      {"tier": 3, "name": "L3-Managers", "typicalRoles": ["Manager", "Lead", "Principal"], "okrLevel": "Team", "accountableTo": "L2"},
      {"tier": 4, "name": "L4-ICs", "typicalRoles": ["Specialist", "Analyst", "Engineer", "Designer"], "okrLevel": "Individual", "accountableTo": "L3"}
    ]
  },

  "ve:DiscoveryQuestions": {
    "@type": "rrrr:RoleDiscoveryFramework",
    "rrrr:phases": [
      {
        "phase": 1,
        "name": "Organization Profiling",
        "questions": [
          {"id": "DP-001", "question": "What is your organization's approximate employee count?", "mappedTo": "OrganizationSize"},
          {"id": "DP-002", "question": "What industry vertical are you in?", "mappedTo": "IndustryVertical"},
          {"id": "DP-003", "question": "What governance model do you follow? (Public, Private, PE-backed, Family-owned, Non-profit)", "mappedTo": "GovernanceModel"},
          {"id": "DP-004", "question": "How would you describe your organization's AI maturity?", "mappedTo": "AIMaturityLevel"}
        ]
      },
      {
        "phase": 2,
        "name": "Role Discovery",
        "questions": [
          {"id": "RD-001", "question": "Who is accountable for overall company strategy?", "expectedPattern": ["CEO", "Managing Director"]},
          {"id": "RD-002", "question": "Who is accountable for financial performance and reporting?", "expectedPattern": ["CFO", "Finance Director", "CEO"]},
          {"id": "RD-003", "question": "Who is accountable for technology strategy and delivery?", "expectedPattern": ["CTO", "CIO", "VP Engineering", "CEO"]},
          {"id": "RD-004", "question": "Who is accountable for revenue and sales?", "expectedPattern": ["CRO", "VP Sales", "CEO", "CMO"]},
          {"id": "RD-005", "question": "Who is accountable for marketing and brand?", "expectedPattern": ["CMO", "VP Marketing", "CEO"]},
          {"id": "RD-006", "question": "Who is accountable for product strategy?", "expectedPattern": ["CPO", "VP Product", "CTO", "CEO"]},
          {"id": "RD-007", "question": "Who is accountable for people and culture?", "expectedPattern": ["CHRO", "VP HR", "COO", "CEO"]},
          {"id": "RD-008", "question": "Who is accountable for operations?", "expectedPattern": ["COO", "VP Operations", "CEO"]},
          {"id": "RD-009", "question": "Who is accountable for data strategy?", "expectedPattern": ["CDO", "CTO", "CIO"]},
          {"id": "RD-010", "question": "Who is accountable for security and risk?", "expectedPattern": ["CISO", "CRO-Risk", "CTO", "CLO"]},
          {"id": "RD-011", "question": "Who is accountable for AI strategy and governance?", "expectedPattern": ["CAIO", "CTO", "CDO"]},
          {"id": "RD-012", "question": "Who is accountable for sustainability/ESG?", "expectedPattern": ["CSustO", "COO", "CEO"]}
        ]
      },
      {
        "phase": 3,
        "name": "Subsumption Detection",
        "questions": [
          {"id": "SD-001", "question": "Does [Role X] also cover responsibilities typically held by [Role Y]?", "detectsPattern": "Subsumption"},
          {"id": "SD-002", "question": "Which specific responsibilities from [Role Y] are covered by [Role X]?", "detectsPattern": "ScopeTransfer"},
          {"id": "SD-003", "question": "Is this a permanent or interim arrangement?", "detectsPattern": "SubsumptionType"},
          {"id": "SD-004", "question": "Who else supports these responsibilities?", "detectsPattern": "Overlap"}
        ]
      }
    ]
  },

  "ve:AgentFunctions": {
    "@type": "pf:AgentFunctionRegistry",
    "functions": [
      {"name": "orgProfileDiscovery", "agent": "RRRDiscoveryAgent", "inputs": ["organizationId", "discoveryContext"], "outputs": ["OrganizationProfile"]},
      {"name": "rolePatternMatch", "agent": "RRRDiscoveryAgent", "inputs": ["OrganizationProfile", "discoveryResponses"], "outputs": ["RoleInstance[]"]},
      {"name": "subsumptionDetect", "agent": "RRRDiscoveryAgent", "inputs": ["RoleInstance[]", "discoveryResponses"], "outputs": ["SubsumptionConfiguration[]"]},
      {"name": "tierCascadeMap", "agent": "RRRDiscoveryAgent", "inputs": ["RoleInstance[]", "OrganizationProfile"], "outputs": ["ManagementTier[]"]},
      {"name": "resolveEffectiveRACI", "agent": "RACIResolutionAgent", "inputs": ["activity", "RoleInstance[]", "SubsumptionConfiguration[]"], "outputs": ["RACIXAssignment[]"]},
      {"name": "validateSingleAccountable", "agent": "RACIResolutionAgent", "inputs": ["RACIXAssignment[]"], "outputs": ["ValidationResult"]},
      {"name": "detectOverlapConflicts", "agent": "RACIResolutionAgent", "inputs": ["RACIXAssignment[]"], "outputs": ["OverlapConflict[]"]},
      {"name": "generateTierOKRs", "agent": "OKRCascadeAgent", "inputs": ["ManagementTier", "parentOKR", "RoleInstance[]"], "outputs": ["OKR[]"]},
      {"name": "validateCascadeAlignment", "agent": "OKRCascadeAgent", "inputs": ["OKR[]", "ManagementTier[]"], "outputs": ["CascadeValidationResult"]},
      {"name": "trackDeliveryAccountability", "agent": "OKRCascadeAgent", "inputs": ["OKR[]", "ManagementTier", "reportingPeriod"], "outputs": ["OKRProgressReport"]}
    ]
  }
}
