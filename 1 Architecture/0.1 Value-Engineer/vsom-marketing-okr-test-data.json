    {
      "rule": "OKR Key Result Cardinality",
      "description": "Every OKR must have 2-5 key results",
      "test": "SELECT ?okr (COUNT(?kr) as ?count) WHERE { ?okr a :OKR . OPTIONAL { ?okr :hasKeyResult ?kr } } GROUP BY ?okr HAVING (?count < 2 || ?count > 5)",
      "expectedResult": "Empty result set"
    },
    {
      "rule": "Progress Percentage Range",
      "description": "Progress percentage must be 0-100",
      "test": "SELECT ?kr ?progress WHERE { ?kr a :KeyResult ; :progressPercent ?progress . FILTER (?progress < 0 || ?progress > 100) }",
      "expectedResult": "Empty result set"
    },
