    "commonPatterns": [
      {
        "name": "Corporate to Execution Cascade",
        "description": "Trace OKRs from corporate strategy → marketing → AI Visibility → execution",
        "example": "Corporate market leadership → Marketing thought leadership → AI discoverability → Content optimization"
      },
      {
        "name": "Dependency Analysis",
        "description": "Identify what marketing activities depend on AI Visibility",
        "example": "Lead generation depends on AI Visibility, which requires Content optimization and Citation building"
      },
      {
        "name": "Platform Implementation",
        "description": "Customize AI Visibility strategy for specific platform",
        "example": "BAIV focuses on thought leadership, W4M on feature education, ECOOAI on product visibility"
      },
      {
        "name": "Cross-Functional Collaboration",
        "description": "Coordinate AI Visibility across C-Suite",
        "example": "CMO owns strategy, CTO implements infrastructure, CDO structures data, CAO integrates AI"
      }
    ]
